QAAQ,GAAG,QAAQ,CAAC,QAAT,CAAkB,QAAQ,CAAC,KAAT,CAAe,QAAjC,EAA2C,KAAK,CAAC,OAAN,CAAc,QAAzD,EAAmE,QAAnE,EAAjB;AACA,MAAA,QAAQ,CAAC,IAAT,CAAc,QAAd,kCAA6B,QAAQ,CAAC,KAAtC;AAA6C,QAAA,QAAQ,EAAR;AAA7C,UAAyD,IAAzD,CAA8D,YAAM;AAClE,QAAA,MAAM,CAAC,IAAP,CAAY;AAAE,UAAA,IAAI,EAAE;AAAR,SAAZ;AACD,OAFD;AAGD,KALD","sourcesContent":["<template>\n  <div class=\"login_register_container\">\n    <h3 class=\"text-2xl text-center mb-3\">Kayıt Ol</h3>\n    <input v-model=\"userData.fullname\" type=\"text\" placeholder=\"Tam Ad\" class=\"input mb-3\" />\n    <input v-model=\"userData.username\" type=\"text\" placeholder=\"Kullanıcı Adı\" class=\"input mb-3\" />\n    <input v-model=\"userData.password\" type=\"password\" placeholder=\"Şifre\" class=\"input mb-3\" />\n    <button class=\"default-button\" @click=\"onSave\">Kayıt ol</button>\n    <span class=\"text-center mt-3 text-sm\">\n      Zaten Üyeyim,\n      <router-link :to=\"{ name: 'LoginPage' }\" class=\"text-red-900 hover:text-black\">\n        Giriş yap!\n      </router-link>\n    </span>\n  </div>\n</template>\n<script setup>\nimport { ref, inject } from \"vue\";\nimport { useStore } from \"vuex\";\nimport { useRouter } from \"vue-router\";\nimport CryptoJS from \"crypto-js\";\nconst appAxios = inject(\"appAxios\");\nconst store = useStore();\nconst router = useRouter();\nconst userData = ref({ username: null, fullname: null, password: null });\nconst onSave = () => {\n  const password = CryptoJS.HmacSHA1(userData.value.password, store.getters._saltKey).toString();\n  appAxios.post(\"/users\", { ...userData.value, password }).then(() => {\n    router.push({ name: \"HomePage\" });\n  });\n};\n</script>\n"]}]}